<%- model_class = Usuario -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @usuario.usuario_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:correo) %>:</strong></dt>
  <dd><%= @usuario.correo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombreUsuario) %>:</strong></dt>
  <dd><%= @usuario.nombreUsuario %></dd>
  <dt><strong><%= model_class.human_attribute_name(:claveUsuario) %>:</strong></dt>
  <dd><%= @usuario.claveUsuario %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @usuario.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:apellido) %>:</strong></dt>
  <dd><%= @usuario.apellido %></dd>
  <dt><strong><%= model_class.human_attribute_name(:genero) %>:</strong></dt>
  <dd><%= @usuario.genero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechaNacimiento) %>:</strong></dt>
  <dd><%= @usuario.fechaNacimiento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:facebook) %>:</strong></dt>
  <dd><%= @usuario.facebook %></dd>
  <dt><strong><%= model_class.human_attribute_name(:twitter) %>:</strong></dt>
  <dd><%= @usuario.twitter %></dd>
  <dt><strong><%= model_class.human_attribute_name(:linkedin) %>:</strong></dt>
  <dd><%= @usuario.linkedin %></dd>
  <dt><strong><%= model_class.human_attribute_name(:youtube) %>:</strong></dt>
  <dd><%= @usuario.youtube %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono_id) %>:</strong></dt>
  <dd><%= @usuario.telefono_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion_id) %>:</strong></dt>
  <dd><%= @usuario.direccion_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              usuarios_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_usuario_path(@usuario), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              usuario_path(@usuario),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
