<%- model_class = Experiencia -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:experiencia_id) %></th>
      <th><%= model_class.human_attribute_name(:fechaInicio) %></th>
      <th><%= model_class.human_attribute_name(:fechaFin) %></th>
      <th><%= model_class.human_attribute_name(:rubro) %></th>
      <th><%= model_class.human_attribute_name(:cargo) %></th>
      <th><%= model_class.human_attribute_name(:descripcion) %></th>
      <th><%= model_class.human_attribute_name(:laboral_id) %></th>
      <th><%= model_class.human_attribute_name(:empresa_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @experiencias.each do |experiencia| %>
      <tr>
        <td><%= link_to experiencia.id, experiencia_path(experiencia) %></td>
        <td><%= experiencia.experiencia_id %></td>
        <td><%= experiencia.fechaInicio %></td>
        <td><%= experiencia.fechaFin %></td>
        <td><%= experiencia.rubro %></td>
        <td><%= experiencia.cargo %></td>
        <td><%= experiencia.descripcion %></td>
        <td><%= experiencia.laboral_id %></td>
        <td><%= experiencia.empresa_id %></td>
        <td><%=l experiencia.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_experiencia_path(experiencia), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      experiencia_path(experiencia),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_experiencia_path,
            :class => 'btn btn-primary' %>
