<%- model_class = Experiencia -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:experiencia_id) %>:</strong></dt>
  <dd><%= @experiencia.experiencia_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechaInicio) %>:</strong></dt>
  <dd><%= @experiencia.fechaInicio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechaFin) %>:</strong></dt>
  <dd><%= @experiencia.fechaFin %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rubro) %>:</strong></dt>
  <dd><%= @experiencia.rubro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cargo) %>:</strong></dt>
  <dd><%= @experiencia.cargo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @experiencia.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:laboral_id) %>:</strong></dt>
  <dd><%= @experiencia.laboral_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:empresa_id) %>:</strong></dt>
  <dd><%= @experiencia.empresa_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              experiencias_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_experiencia_path(@experiencia), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              experiencia_path(@experiencia),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
