<%= form_for @contrato, :html => { :class => "form-horizontal contrato" } do |f| %>

    <% if @contrato.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@contrato.errors.count, "error") %> prohibited this contrato from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @contrato.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="control-group">
    <%= f.label :contrato_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :contrato_id, :class => 'form-control' %>
    </div>
    <%= error_span(@contrato[:contrato_id]) %>
  </div>
  <div class="control-group">
    <%= f.label :prestador, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :prestador, :class => 'form-control' %>
    </div>
