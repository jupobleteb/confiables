<%- model_class = Contrato -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:contrato_id) %>:</strong></dt>
  <dd><%= @contrato.contrato_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prestador) %>:</strong></dt>
  <dd><%= @contrato.prestador %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @contrato.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @contrato.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechaInicio) %>:</strong></dt>
  <dd><%= @contrato.fechaInicio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechaFin) %>:</strong></dt>
  <dd><%= @contrato.fechaFin %></dd>
  <dt><strong><%= model_class.human_attribute_name(:clausulaTermino) %>:</strong></dt>
  <dd><%= @contrato.clausulaTermino %></dd>
  <dt><strong><%= model_class.human_attribute_name(:evalucion) %>:</strong></dt>
  <dd><%= @contrato.evalucion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:valorservicio) %>:</strong></dt>
  <dd><%= @contrato.valorservicio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comision) %>:</strong></dt>
  <dd><%= @contrato.comision %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @contrato.usuario_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              contratos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_contrato_path(@contrato), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              contrato_path(@contrato),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
