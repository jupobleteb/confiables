<%- model_class = Sla -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:sla_id) %>:</strong></dt>
  <dd><%= @sla.sla_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @sla.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @sla.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:porcentajeImportacia) %>:</strong></dt>
  <dd><%= @sla.porcentajeImportacia %></dd>
  <dt><strong><%= model_class.human_attribute_name(:activo) %>:</strong></dt>
  <dd><%= @sla.activo %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              slas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_sla_path(@sla), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              sla_path(@sla),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
