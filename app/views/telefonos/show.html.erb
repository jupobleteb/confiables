<%- model_class = Telefono -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:telefono_id) %>:</strong></dt>
  <dd><%= @telefono.telefono_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codpais) %>:</strong></dt>
  <dd><%= @telefono.codpais %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codciudad) %>:</strong></dt>
  <dd><%= @telefono.codciudad %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero) %>:</strong></dt>
  <dd><%= @telefono.numero %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              telefonos_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_telefono_path(@telefono), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              telefono_path(@telefono),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
