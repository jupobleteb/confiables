<%- model_class = Ubicacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:ubicacion_id) %>:</strong></dt>
  <dd><%= @ubicacion.ubicacion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitud) %>:</strong></dt>
  <dd><%= @ubicacion.longitud %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitud) %>:</strong></dt>
  <dd><%= @ubicacion.latitud %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fecha) %>:</strong></dt>
  <dd><%= @ubicacion.fecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @ubicacion.usuario_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              ubicaciones_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_ubicacion_path(@ubicacion), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              ubicacion_path(@ubicacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
