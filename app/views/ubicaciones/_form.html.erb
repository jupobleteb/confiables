<%= form_for @ubicacion, :html => { :class => "form-horizontal ubicacion" } do |f| %>

    <% if @ubicacion.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@ubicacion.errors.count, "error") %> prohibited this ubicacion from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @ubicacion.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="control-group">
    <%= f.label :ubicacion_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ubicacion_id, :class => 'form-control' %>
    </div>
    <%= error_span(@ubicacion[:ubicacion_id]) %>
  </div>
  <div class="control-group">
    <%= f.label :longitud, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :longitud, :class => 'form-control' %>
    </div>
    <%= error_span(@ubicacion[:longitud]) %>
  </div>
  <div class="control-group">
    <%= f.label :latitud, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :latitud, :class => 'form-control' %>
    </div>
    <%= error_span(@ubicacion[:latitud]) %>
  </div>
  <div class="control-group">
    <%= f.label :fecha, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fecha, :class => 'form-control' %>
    </div>
    <%= error_span(@ubicacion[:fecha]) %>
  </div>
  <div class="control-group">
    <%= f.label :usuario_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :usuario_id, :class => 'form-control' %>
    </div>
    <%= error_span(@ubicacion[:usuario_id]) %>
  </div>


  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
            ubicaciones_path, :class => 'btn btn-default' %>

<% end %>
