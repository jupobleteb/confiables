<%- model_class = Empresa -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:rut) %>:</strong></dt>
  <dd><%= @empresa.rut %></dd>
  <dt><strong><%= model_class.human_attribute_name(:nombre) %>:</strong></dt>
  <dd><%= @empresa.nombre %></dd>
  <dt><strong><%= model_class.human_attribute_name(:acronimo) %>:</strong></dt>
  <dd><%= @empresa.acronimo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rubro) %>:</strong></dt>
  <dd><%= @empresa.rubro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rutrepresentante) %>:</strong></dt>
  <dd><%= @empresa.rutrepresentante %></dd>
  <dt><strong><%= model_class.human_attribute_name(:email) %>:</strong></dt>
  <dd><%= @empresa.email %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono_id) %>:</strong></dt>
  <dd><%= @empresa.telefono_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion_id) %>:</strong></dt>
  <dd><%= @empresa.direccion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @empresa.usuario_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              empresas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_empresa_path(@empresa), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              empresa_path(@empresa),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
