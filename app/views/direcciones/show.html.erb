<%- model_class = Direccion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:direccion_id) %>:</strong></dt>
  <dd><%= @direccion.direccion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion) %>:</strong></dt>
  <dd><%= @direccion.direccion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero) %>:</strong></dt>
  <dd><%= @direccion.numero %></dd>
  <dt><strong><%= model_class.human_attribute_name(:complemento) %>:</strong></dt>
  <dd><%= @direccion.complemento %></dd>
  <dt><strong><%= model_class.human_attribute_name(:codPostal) %>:</strong></dt>
  <dd><%= @direccion.codPostal %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comuna_id) %>:</strong></dt>
  <dd><%= @direccion.comuna_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              direcciones_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_direccion_path(@direccion), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              direccion_path(@direccion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
