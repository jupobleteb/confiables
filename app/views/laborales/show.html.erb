<%- model_class = Laboral -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:datoslaborales_id) %>:</strong></dt>
  <dd><%= @laboral.datoslaborales_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:correo) %>:</strong></dt>
  <dd><%= @laboral.correo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:telefono_id) %>:</strong></dt>
  <dd><%= @laboral.telefono_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fechacontrato) %>:</strong></dt>
  <dd><%= @laboral.fechacontrato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @laboral.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:direccion_id) %>:</strong></dt>
  <dd><%= @laboral.direccion_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:usuario_id) %>:</strong></dt>
  <dd><%= @laboral.usuario_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:empresa_id) %>:</strong></dt>
  <dd><%= @laboral.empresa_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              laborales_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_laboral_path(@laboral), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              laboral_path(@laboral),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
