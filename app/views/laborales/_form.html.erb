<%= form_for @laboral, :html => { :class => "form-horizontal laboral" } do |f| %>

    <% if @laboral.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@laboral.errors.count, "error") %> prohibited this laboral from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @laboral.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="control-group">
    <%= f.label :datoslaborales_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :datoslaborales_id, :class => 'form-control' %>
    </div>
    <%= error_span(@laboral[:datoslaborales_id]) %>
  </div>
  <div class="control-group">
    <%= f.label :correo, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :correo, :class => 'form-control' %>
    </div>
    <%= error_span(@laboral[:correo]) %>
  </div>
  <div class="control-group">
    <%= f.label :telefono_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :telefono_id, :class => 'form-control' %>
    </div>
    <%= error_span(@laboral[:telefono_id]) %>
  </div>
  <div class="control-group">
    <%= f.label :fechacontrato, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :fechacontrato, :class => 'form-control' %>
    </div>
    <%= error_span(@laboral[:fechacontrato]) %>
  </div>
  <div class="control-group">
    <%= f.label :descripcion, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :descripcion, :class => 'form-control' %>
