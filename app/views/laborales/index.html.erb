<%- model_class = Laboral -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:datoslaborales_id) %></th>
      <th><%= model_class.human_attribute_name(:correo) %></th>
      <th><%= model_class.human_attribute_name(:telefono_id) %></th>
      <th><%= model_class.human_attribute_name(:fechacontrato) %></th>
      <th><%= model_class.human_attribute_name(:descripcion) %></th>
      <th><%= model_class.human_attribute_name(:direccion_id) %></th>
      <th><%= model_class.human_attribute_name(:usuario_id) %></th>
      <th><%= model_class.human_attribute_name(:empresa_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @laborales.each do |laboral| %>
      <tr>
        <td><%= link_to laboral.id, laboral_path(laboral) %></td>
        <td><%= laboral.datoslaborales_id %></td>
        <td><%= laboral.correo %></td>
        <td><%= laboral.telefono_id %></td>
        <td><%= laboral.fechacontrato %></td>
        <td><%= laboral.descripcion %></td>
        <td><%= laboral.direccion_id %></td>
        <td><%= laboral.usuario_id %></td>
        <td><%= laboral.empresa_id %></td>
        <td><%=l laboral.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_laboral_path(laboral), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      laboral_path(laboral),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_laboral_path,
            :class => 'btn btn-primary' %>
